   "source": [
    "pattern = \"\"\" (?P<fullname>.+)    # Group 1: Full name\n",
    "              (?::\\s)             # Non-captured group\n",
    "              (?P<grades>\\w)      # Group 2: Grades\n",
    "              (?:\\s)              # Non-captured group\n",
